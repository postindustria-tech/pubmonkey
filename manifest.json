{
    "manifest_version": 2,
 
    "name": "Mopub-automation",
    "version": "0.1",    
    "icons": {
            "16": "images/empty_16.png",
            "32": "images/empty_32.png",
            "48": "images/empty_48.png",
            "64": "images/empty_64.png",
            "128": "images/empty_128.png"
    },
 
    "content_scripts": [
        {
            "matches": [ "https://app.mopub.com/*","http://app.mopub.com/*"],
            "js": [ "content_scripts/end.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": ["http://*/","https://*/"],
    "browser_action": {
          "default_icon": {
            "16": "images/empty_16.png",
            "32": "images/empty_32.png",
            "48": "images/empty_48.png",
            "64": "images/empty_64.png",
            "128": "images/empty_128.png"
          },
          "default_title": "Mopub-automation",
          "default_popup": "popup/popup.html"
    }
}
